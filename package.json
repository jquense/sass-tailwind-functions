{
  "name": "sass-tailwind-functions",
  "version": "1.1.0",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jquense/sass-tailwind-functions.git"
  },
  "author": "Jason Quense",
  "license": "MIT",
  "scripts": {
    "format": "4c format src './*'",
    "lint": "4c lint src './*'",
    "release": "4c release",
    "tdd": "jest --watch",
    "test": "yarn lint && for alias in sass sass-1.11; do echo \"\ntesting $alias\n\"; SASS_ALIAS=$alias jest; done",
    "testonly": "jest"
  },
  "publishConfig": {
    "access": "public"
  },
  "prettier": "@4c/prettier-config",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*": [
      "yarn 4c lint --fix",
      "git add"
    ]
  },
  "jest": {
    "preset": "@4c/jest-preset",
    "testEnvironment": "node"
  },
  "release": {
    "conventionalCommits": true
  },
  "devDependencies": {
    "@4c/cli": "^2.0.1",
    "@4c/jest-preset": "^1.4.5",
    "@4c/prettier-config": "^1.1.0",
    "eslint-config-4catalyzer": "^1.1.0",
    "eslint-config-4catalyzer-jest": "^2.0.4",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.1.2",
    "hookem": "^1.0.6",
    "jest": "^27.0.4",
    "lint-staged": "^11.0.0",
    "prettier": "^2.0.1",
    "sass": "^1.44",
    "sass-1.11": "npm:sass@1.11.0",
    "tailwindcss": "^2.2.7"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "sass-cast": "^0.5.2"
  },
  "peerDependencies": {
    "sass": "^1.11.0",
    "tailwindcss": ">=2.2.7"
  }
}
